        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "7.2.1",
      "targets": [
        {
          "expr": "(\n  (\n    sum(increase(changelog_prom_ex_phoenix_http_request_duration_milliseconds_bucket{kubernetes_namespace=\"$namespace\", le=\"256\", status=~\"2..\"}[24h])) + \n    (sum(increase(changelog_prom_ex_phoenix_http_request_duration_milliseconds_bucket{kubernetes_namespace=\"$namespace\", le=\"1024\", status=~\"2..\"}[24h])) - sum(increase(changelog_prom_ex_phoenix_http_request_duration_milliseconds_bucket{kubernetes_namespace=\"$namespace\", le=\"256\", status=~\"2..\"}[24h]))) / 2\n  ) \n  / \n  sum(increase(changelog_prom_ex_phoenix_http_request_duration_milliseconds_count{kubernetes_namespace=\"$namespace\", status=~\"2..\"}[24h]))\n) * 100",
